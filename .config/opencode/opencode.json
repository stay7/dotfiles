{
  "plugin": [
    "oh-my-opencode",
    "opencode-openai-codex-auth",
    "opencode-anthropic-auth@latest",
    "opencode-google-antigravity-auth"
  ],
  "disabled_providers": ["opencode"],
  "provider": {
    "anthropic": {
      "name": "Anthropic",
      "models": {
        "claude-opus-4-5": {
          "name": "Claude Opus 4.5",
          "limit": {
            "context": 200000,
            "output": 32000
          }
        },
        "claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5",
          "limit": {
            "context": 200000,
            "output": 16000
          }
        },
        "claude-haiku-4-5": {
          "name": "Claude Haiku 4.5",
          "limit": {
            "context": 200000,
            "output": 8192
          }
        }
      }
    },
    "google": {
      "name": "Google",
      "models": {
        "gemini-3-pro": {
          "name": "Gemini 3 Pro",
          "limit": {
            "context": 1000000,
            "output": 65536
          }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash",
          "limit": {
            "context": 1000000,
            "output": 65536
          }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro",
          "limit": {
            "context": 1000000,
